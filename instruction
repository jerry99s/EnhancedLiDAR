----------------------------------DeepPruner-------------------------------------------------
生成视差图
conda activate python2
python submission_kitti.py \
--loadmodel /home/song/DeepPruner/deeppruner/DeepPruner-best-kitti.tar \
--save_dir /home/song/DeepPruner/deeppruner/test_directory/predicted_disp \
--datapath /home/song/DeepPruner/deeppruner/test_directory
-------------------------------------------------------------------------------------------------

----------------------------------pseudo-LiDAR-------------------------------------------------
生成深度图（由视差图）
python ./preprocessing/generate_depth.py  \
    --calib_dir /media/song/程序磁盘/3D_KITTI/data_object_calib/testing/calib/ \
    --save_dir /media/song/程序磁盘/3D_KITTI/predicted_depth/testing/ \
    --disparity_dir /media/song/程序磁盘/3D_KITTI/predicted_disp/testing/ \
    --start_index 534
